{
  "semi": true,
  "singleQuote": true,
  "importOrder": [
    "^reflect-metadata",
    "^autobind-decorator",
    "^vue",
    "^vue-i18n",
    "^vue-router",
    "^@kaokei/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "#imports",
    "^~/types$",
    "^[~/]",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "classProperties",
    "decorators-legacy"
  ],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
